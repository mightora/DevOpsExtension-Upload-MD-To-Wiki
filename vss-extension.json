{
    "manifestVersion": 1,
    "id": "upload-md-to-wiki",
    "name": "Mightora Upload MD To Wiki",
    "version": "1.1.127",
    "publisher": "mightoraio",
    "galleryFlags": [
        "Public",
        "Preview"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "This DevOps extension allows you to move markdown (.md) files held in an Azure DevOps repository and publish them to an Azure DevOps Wiki. Automate your documentation process and keep your wiki up-to-date with the latest markdown files from your repository.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/mightora_io_icon_260x260.png",
        "note": "images/notes_screen.png",
        "config":"images/note_screen_configs.png"
    },
    "tags": [
        "CI/CD",
        "Notes",
        "Release Notes",
        "Pipeline",
        "WorkItems"
    ],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "links": {
        "home": {
            "uri": "https://mightora.io"
        },
        "getstarted": {
            "uri": "https://mightora.io"
        },
        "learn": {
            "uri": "https://mightora.io"
        },
        "support": {
            "uri": "https://mightora.io"
        },
        "repository": {
            "uri": "https://github.com/mightora/DevOpsExtension-Upload-MD-To-Wiki/"
        },
        "issues": {
            "uri": "https://github.com/mightora/DevOpsExtension-Upload-MD-To-Wiki/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/mightora/DevOpsExtension-Upload-MD-To-Wiki"
    },
    "files": [
        {
            "path": "src"
        }
    ],
    "contributions": [
        {
            "id": "mightora-wiki-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "src"
            }
        }
    ]
}